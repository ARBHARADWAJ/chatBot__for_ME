# Google API Key for chat responses
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE

# Secret for JSON Web Token (JWT) authentication
JWT_SECRET=YOUR_JWT_SECRET_HERE

# Port for the backend server to listen on
PORT=3000

# MongoDB connection URI (e.g., mongodb://user:pass@host:port/dbname)
MONGO_URI=mongodb://root:example@localhost:27017/

# MongoDB database name
MONGO_DB_NAME=test

# CORS allowed origins (comma-separated for multiple origins)
CORS_ORIGIN=http://localhost:5173,http://127.0.0.1:5173

# Redis connection URL (e.g., redis://localhost:6379)
REDIS_URL=redis://localhost:6379
